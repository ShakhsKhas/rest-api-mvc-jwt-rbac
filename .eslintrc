{
  "extends": "airbnb-base",
  "rules": {
    // windows linebreaks when not in production environment
    "linebreak-style": ["error", "windows"],
    "indent": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "no-cond-assign": 0,
    "no-param-reassign": 0,
    "object-curly-newline": 0,
    "eqeqeq": 0
  }
}